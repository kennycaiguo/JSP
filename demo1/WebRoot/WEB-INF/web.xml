<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

  <servlet>
    <servlet-name>UserLogin</servlet-name>
    <servlet-class>cn.svse.servlet.user.UserLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HouseSave</servlet-name>
    <servlet-class>cn.svse.servlet.house.HouseSave</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HouseList</servlet-name>
    <servlet-class>cn.svse.servlet.house.HouseList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>HouseEditUI</servlet-name>
    <servlet-class>cn.svse.servlet.house.HouseEditUI</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/servlet/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HouseSave</servlet-name>
    <url-pattern>/servlet/HouseSave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HouseList</servlet-name>
    <url-pattern>/servlet/HouseList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HouseEditUI</servlet-name>
    <url-pattern>/servlet/HouseEditUI</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
